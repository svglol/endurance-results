{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "migrate": "drizzle-kit generate:sqlite",
    "drizzle-studio": "drizzle-kit studio",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare && pnpm simple-git-hooks install",
    "lint": "pnpm lint:js && pnpm lint:style",
    "lint:js": "eslint --ext .js,.ts,.mjs,.vue --fix --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "preview": "nuxt preview",
    "test": "vitest --run",
    "typecheck": "nuxt typecheck"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint --edit $1"
  },
  "lint-staged": {
    "*.{js,ts,mjs,vue}": [
      "eslint --fix"
    ],
    "*.{css,vue}": "stylelint --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@libsql/client": "0.5.6",
    "@nuxt/devtools": "1.1.4",
    "@nuxt/eslint-config": "0.2.0",
    "@nuxt/fonts": "^0.5.0",
    "@nuxt/test-utils": "3.12.0",
    "@nuxt/ui": "2.15.0",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "@nuxtjs/seo": "2.0.0-rc.10",
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "@upstash/qstash": "^2.3.0",
    "@vite-pwa/nuxt": "0.6.0",
    "@vue-macros/nuxt": "1.9.26",
    "@vue/test-utils": "2.4.5",
    "@vueuse/core": "10.9.0",
    "@vueuse/nuxt": "10.9.0",
    "drizzle-kit": "0.20.14",
    "drizzle-orm": "0.30.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "9.24.0",
    "happy-dom": "13.10.1",
    "lint-staged": "15.2.2",
    "node-html-parser": "6.1.12",
    "nuxt": "3.10.3",
    "postcss-html": "1.6.0",
    "prettier": "3.2.5",
    "simple-git-hooks": "2.11.1",
    "slugify": "1.6.6",
    "stylelint": "16.3.1",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "36.0.0",
    "vitest": "1.4.0"
  },
  "packageManager": "pnpm@8.15.5"
}
